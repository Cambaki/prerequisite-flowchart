{
  "name": "prerequisite-flowchart",
  "version": "0.1.0",
  "homepage": ".",
  "private": true,
  "dependencies": {
    "lucide-react": "^0.544.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-flow-renderer": "^10.3.17",
    "react-scripts": "5.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "build:esbuild": "esbuild src/index.js --bundle --outfile=dist/bundle.js --loader:.js=jsx --define:process.env.NODE_ENV=\\\"production\\\" --minify",
    "copy:assets": "node scripts/copy-assets.js",
    "build:static": "npm run build:esbuild && npm run copy:assets"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^10.4.16",
    "gh-pages": "^6.1.1",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.5",
    "esbuild": "^0.18.12"
  }
}
